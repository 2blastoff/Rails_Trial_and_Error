<%= form_for @contact, html: {multipart: true} do |f| %>
  <%= f.label :firstname %>
  <%= f.text_field :firstname %>
  <br />
  <%= f.label :surname %>
  <%= f.text_field :surname %>
  <br />
  <%= f.label :email %>
  <%= f.text_field :email %>
  <br />
  <%= f.label :phone %>
  <%= f.text_field :phone %>
  <br />
  <%= f.label :image %>
  <%= f.file_field :image %>
  <br />
  <br />
  <%= f.submit %>
<% end %>
